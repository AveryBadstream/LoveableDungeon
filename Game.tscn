	vec4 mask_c = texture(TEXTURE, UV);
	vec4 wv_c = texture(WorldView, UV);
	vec4 sv_c = texture(ShadowView, UV);
	COLOR = wv_c*mask_c.g + sv_c*mask_c.r;
	//COLOR = wv_c*mask_c.g;
	//COLOR = sv_c;
}"

